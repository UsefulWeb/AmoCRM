{
  "extends": [
    "eslint:recommended",
    "idiomatic"
  ],
  "plugins": [
    "jasmine"
  ],
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "env": {
    "jasmine": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": ["error", "always"]
  },
  "parser": "@babel/eslint-parser"
}
